<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eoneservices.pavan.collegeapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_Activity.LoginPg" />
        <activity
            android:name=".Activities.AdminDrawer"
            android:label="@string/title_activity_admin_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.HodDrawer"
            android:label="@string/title_activity_hod"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.TeacherDrawer"
            android:label="@string/title_activity_teacher_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.StudentDrawer"
            android:label="@string/title_activity_student_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Login_Activity.RegisterPg"></activity>
    </application>

</manifest>